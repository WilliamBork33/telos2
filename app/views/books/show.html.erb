<% provide(:title, "New Sell") %>
<h1>Book Submitted to Sell</h1>

<table class="table is-bordered is-striped is-narrow">
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Author</th>
    <th>Seller ID</th>
    <th>Book ID</th>
    <th>Price</th>
    <th>Date of Listing</th>
  </tr>
 
  <tr>
    <td><%= @book.title %></td>
    <td><%= @book.description %></td>
    <td><%= @book.author %></td>
    <td><%= @user_id %></td>
    <td><%= @book.id %></td>
    <td><%= @book.price %></td>
    <td><%= @book.created_at.strftime("%b. %d %Y") %></td>
  </tr>
</table>

