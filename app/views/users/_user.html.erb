<table class="table is-bordered is-striped is-narrow">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Address</th>
    <th>Phone</th>
    <th>User Since</th>
  </tr>
<% @users.each do |user| %> 
  <tr>
    <td><%= link_to user.name, user %></td>
    <td><%= user.email %></td>
    <td><%= user.address %></td>
    <td><%= user.phone %></td>
    <td><%= user.created_at.strftime("%b. %d %Y") %></td>
  </tr>
<% end %>
</table>
